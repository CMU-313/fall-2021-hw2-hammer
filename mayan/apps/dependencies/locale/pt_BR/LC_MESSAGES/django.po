msgstr "Grupo %s não encontrado."

#: views.py:57
msgid "Dependency groups"
msgstr "Grupos de dependências"

#: views.py:77
#, python-format
msgid "Dependency group and entry: %(group)s, %(entry)s"
msgstr "Grupo de dependências e entrada: %(group)s, %(entry)s"

